@model List<KidKinder.Entities.Teacher>
<div class="container-fluid pt-5">
    <div class="container">
        <div class="text-center pb-2">
            <p class="section-title px-5">
                <span class="px-2">Öğretmenlerimiz</span>
            </p>
            <h1 class="mb-4">Öğretmenlerimizle Tanışın</h1>
        </div>
       
        <div class="row">
            @foreach (var x in Model)
            {
                <div class="col-md-6 col-lg-3 text-center team mb-5">
                    <div class="position-relative overflow-hidden mb-4"
                         style="border-radius: 100%">
                        <img class="img-fluid w-100" src="@x.ImageUrl" alt="" />
                        
                    </div>
                    <h4>@x.NameSurname</h4>
                    <i>@x.Branch.Name</i>
                </div>
            }
            </div>
      
    </div>
       
</div>
